<%= simple_form_for(@receipt) do |f| %>
  <%= f.input :store %>
  <%= f.input :date %>
  <%= f.input :amount %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :category_name, collection: ['shopping', 'restaurant', 'food_and_beverage', 'entertainment', 'transport', 'others'] %>
  <%= image_tag @receipt.photo %>

  <%= f.submit :submit, class: ' btn-main btn-sm ml-2 mt-2 mb-4' %>

<% end %>
